{
  "name": "my-first-website",
  "version": "1.0.0",
  "description": "A simple first heroku app",
  "author": "Jules Walzer-Goldfeld",
  "keywords": [
    "heroku",
    "server",
    "socket",
    "io",
    "tic tac toe"
  ],
  "scripts": {
