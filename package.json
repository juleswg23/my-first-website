  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jquery": "^3.5.14",
    "@types/socket.io": "^3.0.2",
    "copyfiles": "^2.4.1",
    "rimraf": "^3.0.2",
    "socket.io-client": "^4.5.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/juleswg23/my-first-website"
